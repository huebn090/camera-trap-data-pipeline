# Parameters
ID=test_run_GHmodel_MSIcode_originalImages
SITE=SER
SEASON=SER_S11
ROOT=/home/packerc/shared/machine_learning/
CODE=${ROOT}code/deep_learning_for_camera_trap_images/phase1
LOG_DIR=${ROOT}data/models/${SITE}/empty_or_not/phase1/
PREDICTIONS=${ROOT}data/predictions/empty_or_not/${SITE}/${SEASON}/predictions_${ID}.txt
IMAGES_ROOT=/home/packerc/shared/albums//${SITE}/
DATA_INFO=${ROOT}data/info_files/${SITE}/${SEASON}_empty.csv

# Log Parameters
echo "ID: $ID"
echo "SITE: $SITE"
echo "SEASON: $SEASON"
echo "ROOT: $ROOT"
echo "CODE: $CODE"
echo "LOG_DIR: $LOG_DIR"
echo "PREDICTIONS: $PREDICTIONS"
echo "IMAGES_ROOT: $IMAGES_ROOT"
echo "DATA_INFO: $DATA_INFO"

# Load Tensroflow
module load tensorflow/1.4_gpu

# Execute code
cd $CODE

python eval.py --log_dir $LOG_DIR \
--save_predictions $PREDICTIONS \
--architecture vgg \
--data_info $DATA_INFO \
--path_prefix $IMAGES_ROOT
